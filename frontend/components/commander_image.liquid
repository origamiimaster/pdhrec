{% if commander.commanders.length == 1 %}
  <a href="/commander/{{ commander.commanderstring }}" class="gallery-item">
    <img
      loading="lazy"
      alt="{{commander.commanders[0] | escape}}"
      src="{{commander.urls[0]}}">
    <div class="info">In {{ commander.count }} decks</div>
  </a>
{% endif %}
{% if commander.commanders.length == 2 %}
  <a href="/commander/{{commander.commanderstring}}" class="gallery-item">
    <div class="partners">
      <img
        alt="spacer"
        src="data:image/png;base64,"
        class="partner3">
      <img
        loading="lazy"
        alt="{{commander.commanders[0] | escape }}"
        class="partner1"
        src="{{commander.urls[0]}}"
        data-src="{{commander.urls[0]}}">
      <img
        loading="lazy"
        alt="{{commander.commanders[1] | escape }}"
        class="partner2"
        src="{{commander.urls[1]}}"
        data-src="{{commander.urls[1]}}">
      <div class="info">In {{ commander.count }} decks</div>
    </div>
  </a>
{% endif %}